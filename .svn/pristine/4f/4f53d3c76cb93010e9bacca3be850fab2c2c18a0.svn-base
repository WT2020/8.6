package com.hdo.WarehouseDroid.bean;

import java.io.Serializable;

/**
 * description 出入库物品类
 * author 张建银
 * version 1.0
 * Created on 2017/12/15.
 */

public class Thing implements Serializable {

    private BaseThing goods;//物品
    private int num;//数量

    public BaseThing getGoods() {
        return goods;
    }

    public void setGoods(BaseThing goods) {
        this.goods = goods;
    }

    public int getNum() {
        return num;
    }

    public void setNum(int num) {
        this.num = num;
    }
}
